<%- include("../partials/header") %>
    <p>Do you wish to delete this record?</p>
    <form action="/user/delete/<%= data._id %>" method="post" name="UserDelete">
        <fieldset>
        <Table>            
            <tr>
                <td><label>First Name: </label></td>
                <td><label><%=data.firstname %></label></td>
            </tr>
            <tr>
                <td><label>Last Name: </label></td>
                <td><label><%=data.lastname %></label></td>
            </tr>
            <tr>
                <td><label>Address: </label></td>
                <td><label><%=data.address %></label></td>
            </tr>
            <tr>
                <td><label>Type: </label></td>
                <td><label><%=data.type %></label></td>
            </tr>
        </Table>
        </fieldset>
        <button type="submit" class="btn btn-info">Delete</button>
    </form>
<%- include("../partials/footer") %>